<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root spacing="5" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.172-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <VBox spacing="5">
        <Label text="Description"></Label>
        <TextArea fx:id="description" prefHeight="100" prefWidth="250"/>
    </VBox>
    <VBox spacing="5">
        <Label text="Notes"></Label>
        <TextArea fx:id="notes" prefHeight="100" prefWidth="250"/>
    </VBox>
    <VBox fx:id="roomColumn" HBox.hgrow="ALWAYS" spacing="5">
        <Label text="spacing label" visible="false"></Label>
    </VBox>
</fx:root>
